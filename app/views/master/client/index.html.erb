<table class="data display datatable" id="example">
					<thead>
						<tr>
							<th>S.No</th>
							<th>Client ID</th>
							<th>Client Name</th>
							<th>Mobile Number</th>
							<th>Edit</th>
							<th>Delete</th>
						</tr>
					</thead>
					<tbody>
						<% s_no = 0%>
                       <% @client.each do |client|%>
					     <% s_no += 1%>
					     <tr class="<%= cycle("odd gradeX", "even gradeC") %>">
					     	<td><%= s_no %></td>
					     	<td><%= client.name%></td>
							<td><%#= address.email%></td>
							<td><%= link_to 'Edit User', edit_master_client_path(client) %></td>
							<td><%= link_to 'Remove User', master_client_destroy_client_path(client.id), :confirm => 'Are you sure?' %></td>
					     </tr>
						 
					   <% end %>
					</tbody>
				</table>
				
				<div id="example"><b><%= link_to 'Create New Client', new_master_client_path %></b> </div>
